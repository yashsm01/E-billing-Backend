<!doctype html>
<html lang="en">

<head>
	<!-- Required meta tags -->
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

	<!-- Bootstrap CSS -->
	<!-- <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous"> -->
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"
		integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
	<style>
		body {
			font-size: 7px;
			margin-left: 70px;
		}

		.header-s {
			font-size: 7px;
			padding-left: 0px;
			padding-right: 0px;
			padding-top: 5px;
		}

		.qr-img {
			width: 50px;
			height: 50px;
		}

		.gsp-logo {
			width: 40px !important;
			height: 15px;
			margin-right: 4px;
			margin-top: 7px;
		}

		.common {
			padding: 0px;
		}

		.seperator {
			margin-bottom: 55px;
			margin-top: 0px;
		}

		.seperator1 {
			margin-bottom: 0px;
		}
	</style>
</head>

<body>
	<div class="row">
		<% let mod=10; %>
			<% for(let i=0; i < value.length; i=i+2) { %>
				<% if (mod==i) { %>
					<div class="col-xs-6 seperator1">
						<% } else { %>
							<div class="col-xs-6 seperator">
								<% } %>
									<div class="row">
										<div class="col-xs-2" style="padding: 0px !important;">
											<img src="<%= value[i].gspLogo %>" alt="" class="gsp-logo"></img>
										</div>
										<div class="col-xs-10 header-s pt-2">
											<div><b>Willowood Chemicals Pvt. Ltd.</b></div>
											<div><b>
													<%= value[i].unique_code %>
												</b></div>
										</div>
									</div>
									<br />
									<div class="row">
										<div class="col-xs-7">
											<div class="row">
												<div class="col-xs-4 common">SKU ID</div>
												<div class="col-xs-8 common">: &nbsp; <%= value[i].sku %>
												</div>
											</div>
											<div class="row">
												<div class="col-xs-4 common">Batch No</div>
												<div class="col-xs-8 common">: &nbsp; <%= value[i].batchNo %>
												</div>
											</div>
											<div class="row">
												<div class="col-xs-4 common">Mfg. Date</div>
												<div class="col-xs-8 common">: &nbsp; <%= value[i].mfgDate %>
												</div>
											</div>
											<div class="row">
												<div class="col-xs-4 common">Exp. Date</div>
												<div class="col-xs-8 common">: &nbsp; <%= value[i].expDate %>
												</div>
											</div>
											<div class="row">
												<div class="col-xs-4 common">Quantity</div>
												<div class="col-xs-8 common">: &nbsp; <%= value[i].fst_inner_size %>
												</div>
											</div>
											<div class="row">
												<div class="col-xs-4 common">MRP</div>
												<div class="col-xs-8 common">: &nbsp; <%= value[i].mrp %>
												</div>
											</div>
										</div>
										<div class="col-xs-4">
											<img src="<%= value[i].code_image %>" class="qr-img" alt=""></img>
										</div>
									</div>
							</div>


							<% if((i+1) < value.length) { %>

								<% if (mod==i) { mod +=12; %>
									<div class="col-xs-6 seperator1">
										<% } else { %>
											<div class="col-xs-6 seperator">
												<% } %>

													<div class="row">
														<div class="col-xs-2" style="padding:0px !important">
															<img src="<%= value[i].gspLogo %>" alt=""
																class="gsp-logo"></img>
														</div>
														<div class="col-xs-10 header-s">
															<div><b>Willowood Chemicals Pvt. Ltd.</b></div>
															<div><b>
																	<%= value[i+1].unique_code %>
																</b></div>
														</div>
													</div>
													<br />

													<div class="row">
														<div class="col-xs-7">
															<div class="row">
																<div class="col-xs-4 common">SKU ID</div>
																<div class="col-xs-8 common">: &nbsp; <%=
																		value[i+1].sku %>
																</div>
															</div>
															<div class="row">
																<div class="col-xs-4 common">Batch No</div>
																<div class="col-xs-8 common">: &nbsp; <%=
																		value[i+1].batchNo %>
																</div>
															</div>
															<div class="row">
																<div class="col-xs-4 common">Mfg. Date</div>
																<div class="col-xs-8 common">: &nbsp; <%=
																		value[i+1].mfgDate %>
																</div>
															</div>
															<div class="row">
																<div class="col-xs-4 common">Exp. Date</div>
																<div class="col-xs-8 common">: &nbsp; <%=
																		value[i+1].expDate %>
																</div>
															</div>
															<div class="row">
																<div class="col-xs-4 common">Quantity</div>
																<div class="col-xs-8 common">: &nbsp; <%=
																		value[i+1].fst_inner_size %>
																</div>
															</div>
															<div class="row">
																<div class="col-xs-4 common">MRP</div>
																<div class="col-xs-8 common">: &nbsp; <%= value[i+1].mrp %>
																</div>
															</div>
														</div>
														<div class="col-xs-4">
															<img src="<%= value[i+1].code_image %>" alt=""
																class="qr-img"></img>
														</div>
													</div>
											</div>

											<% } %>
												<% } %>
									</div>
</body>

</html>